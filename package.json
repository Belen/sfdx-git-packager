{
  "name": "sfdx-git-packager",
  "description": "Generates a package.xml for difference between two branches",
  "version": "0.0.1",
  "author": "Charlie Jonas @ChuckJonas",
  "bugs": "https://github.com/ChuckJonas/sfdx-git-diff-to-pkg/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^1.4.1",
    "@salesforce/core": "^1.3.2",
    "diff": "^4.0.1",
    "glob-regex": "^0.3.2",
    "ignore": "^5.1.4",
    "rimraf": "^3.0.0",
    "tmp": "^0.1.0",
    "tslib": "^1",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4",
    "@types/rimraf": "^2.0.2",
    "@types/diff": "^4.0.2",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/tmp": "^0.1.0",
    "@types/xml2js": "^0.4.5",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ChuckJonas/sfdx-git-diff-to-pkg",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "git": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ChuckJonas/sfdx-git-diff-to-pkg",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
